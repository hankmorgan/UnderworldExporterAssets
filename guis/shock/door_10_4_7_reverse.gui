windowDef Desktop
{
	rect 0,0,640,480
	nocursor 1
	float doorstate 0
	float lockstate 0
		windowdef panel
		{
		rect 0,0,640,480
		visible 1
		background "guis/assets/shock/doors/2438_reverse/2438_0000";
                
                //Loop on Closed frame
		onTime 0{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0000";
		}

                onTime 50{
			if ("gui::doorstate"==0)
			{resetTime "0";}
			else
			//Start Opening Sequence
			{set "background" "guis/assets/shock/doors/2438_reverse/2438_0001";}
		}

                //Door Opening sequence
		onTime 100{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0002";
		}

               onTime 150{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0003";
		}

                onTime 200{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0004";
		}

                onTime 250{     //Door Opened
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0005";
		}


                onTime 300{ //Loop on Open door
			if ("gui::doorstate"==1)
			{resetTime "250";}
			else
			//Start closing Sequence
			{set "background" "guis/assets/shock/doors/2438_reverse/2438_0005";}
                }



                onTime 350{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0004";
		}

                onTime 400{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0003";
		}
		
                onTime 450{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0002";
		}

                onTime 500{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0001";
		}
		
                onTime 550{
			set "background" "guis/assets/shock/doors/2438_reverse/2438_0000";
		}
		
                onTime 600{ //return to closed loop
			resetTime "0";
		}
	}
}